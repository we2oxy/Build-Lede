uci -q batch <<-EOF
	delete system.ntp.server
	add_list system.ntp.server="time1.cloud.tencent.com" 
	add_list system.ntp.server="time2.cloud.tencent.com" 
	add_list system.ntp.server="time3.cloud.tencent.com"
	add_list system.ntp.server="time4.cloud.tencent.com"
	add_list system.ntp.server="time5.cloud.tencent.com"
	add_list system.ntp.server="time.apple.com"
	add_list system.ntp.server="time.asia.apple.com"
	add_list system.ntp.server="ntp.aliyun.com"
	add_list system.ntp.server="cn.ntp.org.cn"
EOF
uci commit system

uci set network.lan=interface
uci set network.lan.delegate='0'
uci set network.lan.ip6assign='0'
uci set network.lan.ipaddr='10.0.0.99'
uci set network.lan.netmask='255.255.255.0'
uci set network.lan.dns='10.0.0.99'
uci set network.lan.force_link='0'
uci set network.lan.stp='1'
uci set network.lan.igmp_snooping='1'

uci delete network.vpn0
uci delete network.wan6
uci set network.lan.ipv6='0'
uci set network.wan.ipv6='0'
uci -q delete network.globals.ula_prefix
uci commit network


uci set dhcp.@dnsmasq[0]=dnsmasq
uci set dhcp.@dnsmasq[0].domainneeded='1'
uci set dhcp.@dnsmasq[0].localise_queries='1'
uci set dhcp.@dnsmasq[0].local='/lan/'
uci set dhcp.@dnsmasq[0].domain='lan'
uci set dhcp.@dnsmasq[0].expandhosts='1'
uci set dhcp.@dnsmasq[0].authoritative='1'
uci set dhcp.@dnsmasq[0].readethers='1'
uci set dhcp.@dnsmasq[0].leasefile='/tmp/dhcp.leases'
uci set dhcp.@dnsmasq[0].localuse='1'
uci set dhcp.@dnsmasq[0].nonwildcard='0'
uci set dhcp.@dnsmasq[0].filter_aaaa='1'
uci set dhcp.@dnsmasq[0].quietdhcp='1'
uci set dhcp.@dnsmasq[0].sequential_ip='1'
uci set dhcp.@dnsmasq[0].filterwin2k='1'
uci set dhcp.@dnsmasq[0].nonegcache='1'
uci set dhcp.@dnsmasq[0].allservers='1'
uci set dhcp.@dnsmasq[0].localservice='0'
uci set dhcp.@dnsmasq[0].dns_redirect='1'
uci set dhcp.@dnsmasq[0].rebind_protection='0'
uci set dhcp.@dnsmasq[0].nohosts='1'
uci set dhcp.@dnsmasq[0].resolvfile='/tmp/resolv.conf.d/resolv.conf.auto'
uci set dhcp.@dnsmasq[0].cachesize='0'

uci set dhcp.lan=dhcp
uci set dhcp.lan.interface='lan'
uci set dhcp.lan.start='10'
uci set dhcp.lan.leasetime='168h'
uci set dhcp.lan.limit='200'
uci set dhcp.lan.dhcp_option='6,10.0.0.99,223.5.5.5,119.29.29.29'
uci set dhcp.lan.force='1'
uci set dhcp.lan.dhcpv6='disabled'
uci -q delete dhcp.lan.dhcpv6
uci -q delete dhcp.lan.ra
uci commit dhcp

uci set luci.diag.dns='qq.com'
uci set luci.diag.ping='223.5.5.5'
uci set luci.diag.route='223.5.5.5'
uci commit luci


exit 0

